# Isolation: sub_32 only

i1 : bv[16] = in file("experiments/io/inputs/swap_exact_out_reserve_out_hi.in")
i2 : bv[16] = in file("experiments/io/inputs/swap_exact_out_reserve_out_lo.in")
i3 : bv[16] = in file("experiments/io/inputs/swap_exact_out_amount_out_hi.in")
i4 : bv[16] = in file("experiments/io/inputs/swap_exact_out_amount_out_lo.in")
i5 : bv[16] = in file("experiments/io/inputs/swap_exact_out_new_reserve_out_hi.in")
i6 : bv[16] = in file("experiments/io/inputs/swap_exact_out_new_reserve_out_lo.in")

o1 : sbf = out file("experiments/isolation/outputs/sub32_ok.out")

sub_32(a_hi : bv[16], a_lo : bv[16], b_hi : bv[16], b_lo : bv[16], diff_hi : bv[16], diff_lo : bv[16]) := (diff_lo = (a_lo - b_lo)) && (((a_lo < b_lo) -> (diff_hi = (a_hi - b_hi - { #x0001 }:bv[16]))) && (!(a_lo < b_lo) -> (diff_hi = (a_hi - b_hi)))).

r (o1[t]:sbf = 1:sbf <-> sub_32(i1[t], i2[t], i3[t], i4[t], i5[t], i6[t]))
q
