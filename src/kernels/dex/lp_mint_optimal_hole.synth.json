{
  "synth_version": "1.1",
  "backend": { "kind": "sygus", "solver": "cvc5", "timeout_ms": 30000 },
  "scope": { "kind": "inductive", "domain_caps": "from_model" },
  "holes": [
    {
      "hole_id": "lp_mint_expr",
      "kind": "expr",
      "type": "Int",
      "used_in": [
        { "action": "mint", "field": "lp_minted" }
      ],
      "grammar_id": "G_lp_mint",
      "semantic_constraints": [
        "result >= 0",
        "result * reserve0 <= amount0 * lp_supply",
        "result * reserve1 <= amount1 * lp_supply"
      ]
    }
  ],
  "grammars": [
    {
      "grammar_id": "G_lp_mint",
      "start": "Expr",
      "productions": {
        "Prod0": ["(* amount0 lp_supply)"],
        "Prod1": ["(* amount1 lp_supply)"],
        "Q0": ["(safe_div Prod0 reserve0)"],
        "Q1": ["(safe_div Prod1 reserve1)"],
        "Cmp": ["(<= (* amount0 reserve1) (* amount1 reserve0))"],
        "Expr": ["(ite Cmp Q0 Q1)"]
      }
    }
  ],
  "style_profile": {
