{
  "synth_version": "1.1",
  "backend": { "kind": "sygus", "solver": "cvc5", "timeout_ms": 60000 },
  "scope": { "kind": "inductive", "domain_caps": "from_model" },
  "holes": [
    {
      "hole_id": "output_expr",
      "kind": "expr",
      "type": "Int",
      "used_in": [{ "action": "calculate_output", "field": "amount_out" }],
      "grammar_id": "G_output",
      "semantic_constraints": [
        "result >= 0",
        "result < reserve_out"
      ]
    }
  ],
  "grammars": [
    {
      "grammar_id": "G_output",
      "start": "Expr",
      "productions": {
        "Var": ["reserve_in", "reserve_out", "net_in"],
        "Sum": ["(+ reserve_in net_in)"],
        "Prod": ["(* reserve_out net_in)"],
        "Expr": ["(safe_div Prod Sum)", "(safe_div Prod Var)"]
      }
    }
  ],
  "style_profile": {
    "max_nesting": 2,
    "max_branches": 0,
    "max_ast_nodes": 5,
    "penalties": {
      "branch": 10,
      "nesting": 5,
      "ast_node": 1
    }
  },
  "objective": {
    "kind": "lexicographic",
    "goals": ["SATISFY", "MINIMIZE_COST"]
  }
}
