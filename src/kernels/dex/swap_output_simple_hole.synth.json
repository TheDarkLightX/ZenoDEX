{
  "synth_version": "1.1",
  "backend": { "kind": "sygus", "solver": "cvc5", "timeout_ms": 60000 },
  "scope": { "kind": "inductive", "domain_caps": "from_model" },
  "holes": [
    {
      "hole_id": "output_expr",
      "kind": "expr",
      "type": "Int",
      "used_in": [{ "action": "calculate_output", "field": "output_amount" }],
      "grammar_id": "G_output",
      "semantic_constraints": [
        "result > 0",
        "result < reserve_out"
      ]
    }
  ],
  "grammars": [
    {
      "grammar_id": "G_output",
      "start": "Expr",
      "productions": {
        "Const": ["0", "1", "10", "100"],
        "PosConst": ["1", "10", "100"],
        "Term": ["reserve_in", "reserve_out", "input_amount", "(* input_amount Const)", "(* Const reserve_out)", "(* input_amount reserve_out)"],
        "Lin": ["Const", "Term", "(+ Lin Term)", "(- Lin Term)", "(+ reserve_in input_amount)"],
        "Guard": ["(<= Lin Lin)", "(and Guard Guard)"],
        "Expr": ["Lin", "(safe_div Lin PosConst)", "(safe_div Lin Lin)", "(ite Guard Expr Expr)"]
      }
    }
  ],
  "style_profile": {
    "max_nesting": 3,
    "max_branches": 2,
    "max_ast_nodes": 12,
    "penalties": {
      "branch": 10,
      "nesting": 15,
      "ast_node": 1
    }
  },
  "objective": {
    "kind": "lexicographic",
    "goals": ["SATISFY", "MINIMIZE_COST"]
  }
}
