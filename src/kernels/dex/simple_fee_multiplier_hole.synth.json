{
  "synth_version": "1.1",
  "backend": { "kind": "sygus", "solver": "cvc5", "timeout_ms": 30000 },
  "scope": { "kind": "inductive", "domain_caps": "from_model" },
  "holes": [
    {
      "hole_id": "fee_result_expr",
      "kind": "expr",
      "type": "Int",
      "used_in": [{ "action": "calculate", "field": "fee_result" }],
      "grammar_id": "G_fee",
      "semantic_constraints": [
        "result >= 0",
        "result <= fee_amount"
      ]
    }
  ],
  "grammars": [
    {
      "grammar_id": "G_fee",
      "start": "Expr",
      "productions": {
        "Const": ["0", "1", "10", "100", "1000", "10000"],
        "PosConst": ["1", "10", "100", "1000", "10000"],
        "Var": ["fee_amount", "fee_bps"],
        "Term": ["Var", "(* Var Const)", "(* Const Var)", "(* Var Var)"],
        "DivTerm": ["(safe_div Term PosConst)", "(safe_div Term Term)"],
        "Guard": ["(<= Expr Expr)", "(>= Expr Expr)", "(= Expr Expr)"],
        "Expr": ["Const", "Term", "DivTerm", "(+ Expr Expr)", "(- Expr Expr)", "(ite Guard Expr Expr)"]
      }
    }
  ],
  "style_profile": {
    "max_nesting": 2,
    "max_branches": 1,
    "max_ast_nodes": 10,
    "penalties": {
      "branch": 5,
      "nesting": 10,
      "ast_node": 1
    }
  },
  "objective": {
    "kind": "lexicographic",
    "goals": ["SATISFY", "MINIMIZE_COST"]
  }
}
